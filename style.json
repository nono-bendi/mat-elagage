{
  "version": 8,
  "name": "Mat Elagage - Light Sage",
  "sources": {
    "composite": {
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-streets-v8"
    }
  },
  "sprite": "mapbox://sprites/mapbox/streets-v12",
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "center": [6.82, 43.52],
  "zoom": 9.1,
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#f7f9f6"
      }
    },
    {
      "id": "land",
      "type": "fill",
      "source": "composite",
      "source-layer": "land",
      "paint": {
        "fill-color": "#e6efe8"
      }
    },
    {
      "id": "parks",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "paint": {
        "fill-color": "#d8e6d9"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "composite",
      "source-layer": "water",
      "paint": {
        "fill-color": "#dce7f1"
      }
    },
    {
      "id": "roads",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "paint": {
        "line-color": "#b9c4bb",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          8, 0.2,
          14, 1.6
        ]
      }
    },
    {
      "id": "labels",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Medium"],
        "text-size": 12,
        "visibility": "none"
      },
      "paint": {
        "text-color": "#1a1a1a",
        "text-halo-color": "rgba(255,255,255,0.8)",
        "text-halo-width": 1.2
      }
    }
  ]
}
